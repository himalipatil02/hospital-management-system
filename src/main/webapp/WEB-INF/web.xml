  	<servlet-class>com.doctor.servlet.UpdateCommentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>demo</servlet-name>
  	<url-pattern>/demo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AppointmentServlet</servlet-name>
  	<url-pattern>/AppointmentServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateCommentServlet</servlet-name>
  	<url-pattern>/UpdateCommentServlet</url-pattern>
  </servlet-mapping>
